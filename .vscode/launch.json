            "preLaunchTask": "convert",
            "args": ["--target", "lib/main_bazaar.dart" , "--flavor" , "bazaar" ]
        },
        {
            "name": "fruitcraft myket",
            "program": "lib/main_myket.dart",
            "request": "launch",
            "type": "dart",
            "preLaunchTask": "convert",
            "args": ["--target", "lib/main_myket.dart" , "--flavor" , "myket"]
        },
        {
            "name": "fruitcraft google",
            "program": "lib/main_google.dart",
            "request": "launch",
            "type": "dart",
            "preLaunchTask": "convert",
            "args": ["--target", "lib/main_google.dart", "--flavor" , "google" ]
        },
        {
            "name": "fruitcraft (profile mode)",
            "request": "launch",
            "program": "lib/main_bazaar.dart",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "fruitcraft (release mode)",
            "request": "launch",
            "program": "lib/main_myket.dart",
            "type": "dart",
            "flutterMode": "release",
